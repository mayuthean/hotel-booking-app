<%=render "Shared/front_end/banner"%>
<!--Rooms and Suites Area-->
<section class="rooms-suites-area pb-0 section-padding mb-m30">
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-lg-6 col-md-12 col-sm-12 col-xs-12 centered wow fadeInUp" data-wow-delay="0.3s">
        <div class="section-title">
          <h2 class="cl-black">Our Rooms </h2>

        </div>
      </div>
    </div>
    <div class="row">
      <%@rooms.each do |room|%>
          <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12 wow fadeInUp" data-wow-delay="0.4s">
            <div class="single-room-block">
                <img src="<%=room.image%>" alt="">
              <h3 class="text-center"><%=room.room_no%></h3>
              <p>This room has high ceilings, a custom queen bed, bespoke and vintage furnishings.</p>
              <div class="rooms-book-price">
                <%=link_to new_room_booking_path(room),class: "btn btn-primary" do %>
                  Book Now <i class="ti-arrow-right"></i>
                <%end%>
                <span><%=room.price%> <i>/Night</i></span>
              </div>
            </div>
          </div>
      <%end%>
    </div>
  </div>
</section><!--/Rooms and Suites Area-->
